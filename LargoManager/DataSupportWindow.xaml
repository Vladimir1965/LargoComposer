        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="DataSupportWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Generate rhythmic patterns" HorizontalAlignment="Left" Margin="35,34,0,0" 
            VerticalAlignment="Top" Width="165" Click="GenerateRhythmicPatterns" Height="36"/>
        <Button Content="Generate melodic patterns" HorizontalAlignment="Left" Margin="35,75,0,0" 
            VerticalAlignment="Top" Width="165" Click="GenerateMelodicPatterns" Height="36"/>

    </Grid>
</Window>
